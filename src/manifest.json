{
  "name": "Cookie Getter",
  "description": "Get cookies easily",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["activeTab", "cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "assets/icon128.png"
  }
}
